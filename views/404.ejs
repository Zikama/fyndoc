<style>
  .msg_con_ {
    width: 100%;
    height: 400px;
    border-radius: 5px;
    background-color: #ebebeb;
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
  }
  .page-not-found {
    width: 87%;
    height: 43px;
    object-fit: contain;
    font-family: CabinSketch;
    font-size: 60px;
    font-weight: normal;
    font-style: normal;
    font-stretch: normal;
    line-height: 0.53;
    letter-spacing: 6px;
    text-align: center;
    color: #909497;
    margin-top: 5%;
  }
  ._404 {
    width: 80%;
  }
  .simpo_footer {
    width: 100%;
    height: 200px;
    border-radius: 0;
    background-color: #ebebeb;
    padding: 1%;
    display: flex;
    flex-wrap: wrap;
  }
  .simpo_text {
    width: 100%;
    height: auto;
    font-size: 21px;
    font-weight: normal;
    font-style: normal;
    font-stretch: normal;
    line-height: 1.07;
    letter-spacing: 3px;
    color: #07232f;
  }
  .post {
    width: 68%;
    border: 0;
    box-shadow: none;
  }
  .simpo_container {
    display: flex;
    flex-wrap: wrap;
  }
  .simpo_stact {
    overflow: hidden;
    margin: auto;
    margin-bottom: 7px;
    position: relative;
    left: 0;
    bottom: 0;
    width: 100%;
    box-shadow: none;
    border: 0;
  }
  .posts_con {
    min-height: unset;
  }
  .e_btn,
  .h_btn {
    font-size: 31.5px;
    font-weight: 300;
    font-style: normal;
    font-stretch: normal;
    line-height: 1.21;
    letter-spacing: 3.15px;
    text-align: center;
    color: #e9f4fe;
    border: 0;
    cursor: pointer;
  }
  .full_w {
    width: 100%;
    display: flex;
    justify-content: space-around;
  }
  .h_btn {
    width: auto;
    height: auto;
    border-radius: 10px;
    background-color: #a1a1a1;
    width: 100%;
    padding: 5% 28px;
  }
  .e_btn {
    width: auto;
    height: auto;
    border-radius: 10px;
    background-color: #50cb6a;
    width: 100%;
    padding: 5% 38px;
  }
  .Rectangle-2{
    border-radius: 32px;
    box-shadow: 0px 1px 1px 0px rgba(153, 174, 178, 0.69), 5px -28px #ebebeb;
    background: linear-gradient(to right, rgba(255, 255, 255, 0) 4%, white 90%, rgba(255, 255, 255, 0) 93%);
    margin: auto;
    display: flex;
    justify-content: space-between;
    justify-items: center;
    align-items: center;
    padding-right: 2pc;
    box-sizing: border-box;
  }
</style>
<div class="Rectangle-1">
  <% include ./header/index%>

  <div class="art_board">
    <!-- Right sidebar -->
    <div class="posts_con" id="viewport">
      <div class="post">
        <div class="p_body">
          <div class="p_body_content">
            <!-- Inser -->
            <div class="msg_con_">
              <svg
                xmlns="http://www.w3.org/2000/svg"
                width="7.514in"
                height="57.312"
                class="page-not-found"
              >
                <text
                  y="41.141"
                  fill="#909497"
                  font-family="Cabin Sketch"
                  font-size="60"
                >
                  PAGE NOT FOUND
                </text>
              </svg>

              <svg
                xmlns="http://www.w3.org/2000/svg"
                width="660"
                height="2.514in"
                class="_404"
              >
                <text
                  fill="#50cb6a"
                  font-family="Cabin Sketch"
                  font-size="60"
                  transform="matrix(4.29975 0 0 4.29975 .61 177.434)"
                >
                  404
                </text>
              </svg>
            </div>
          </div>
        </div>
      </div>
      <div class="post simpo_stact">
        <div class="p_body">
          <div class="p_body_content">
            <div class="simpo_footer">
              <div class="simpo_container">
                <span class="simpo_text">
                  The page youâ€™re looking for can not be found, if you beleive
                  this is a mistake please contact us
                </span>
                <span class="full_w">
                  <a
                    href="<%=typeof pathToTheRoot !='undefined' ?pathToTheRoot:'' %>"
                  >
                    <button type="button" class="h_btn">Return home</button>
                  </a>
                  <a href="mailto:info@sadjawebsolutions.com"><button type="button" class="e_btn">Email us</button></a>
                </span>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- right sidebar -->
  </div>
</div>

<% include ./modals/custom%>
<script src="<%=typeof pathToTheRoot !=='undefined'?pathToTheRoot:'../'%>assets/js/custom.js"></script>
<script src="<%=typeof pathToTheRoot !=='undefined'?pathToTheRoot:'../'%>assets/js/index.js"></script>
